<DplConfig>
    <DataNode name="/tract_rtd_volume" type="rest" location="http://hostname:port/tract_rtd_volume">
        <Read uriSuffix="get" timeout="60" >
            <TranslateParameters>
                <Parameter name="rtd_id" valueOverride="`dplShell --i cfg/rtdLookupConfig.xml --n rtd_lookup
                                                            --p campaign_id~$(echo %v | sed 's/campaign_id~//' )
                                                        | tail -n +2 | awk '{ print $0; } END{ if( NR == 0 ) print -1; }'
                                                        | tr [:space:] , | sed 's/,$//' &amp;&amp; exit ${PIPESTATUS[0]}`" valueSource="filter" />
                <Parameter name="filterProperties" translatedName="filter" />
            </TranslateParameters>
            <UriQueryParameters>
                <Parameter name="targetApplicableTime" />
                <Group name="filter" >
                    <Parameter name="rtd_id" />
                </Group>
            </UriQueryParameters>
        </Read>
    </DataNode>
</DplConfig>
